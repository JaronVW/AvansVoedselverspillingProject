@model System.Collections.Generic.List<Core.Domain.MealBox>


<h1 class="text-center" >Maaltijdboxen van andere kantines:</h1>

<div asp-validation-summary="All" class="text-danger"></div>
<div class="w-75 mx-auto  mt-5 grid-container mb-5 ">
    @foreach (var m in Model)
    {
        <partial name="MaaltijdBox" model="m"/>
    }
</div>